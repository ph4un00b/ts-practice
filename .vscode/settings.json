{
  "workbench.colorCustomizations": {
    "activityBar.background": "#054b29",
    "titleBar.activeBackground": "#1c743e",
    "titleBar.activeForeground": "#FCFAFE"
  },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.codeActionsOnSave.mode": "all",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.options": {
    "extensinons": [".ts", ".js", ".tsx", ".jsx"]
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "deno.enable": true,
  "deno.unstable": true
}
